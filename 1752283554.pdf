	<style>
		:root {
			--primary-color: #2563eb;
			--secondary-color: #64748b;
			--border-color: #e2e8f0;
			--background-gray: #f8fafc;
			--success-color: #10b981;
			--warning-color: #f59e0b;
		}
		
		body {
			font-family: 'DM Sans', sans-serif;
			line-height: 1.5;
			color: #1e293b;
			margin: 0;
			padding: 0;
			background: #e1e1e1;
			padding: 64px;
			-webkit-print-color-adjust: exact;
		}

		.invoice-container {
			max-width: 800px;
			margin: 0 auto;
			background: #fff;
			box-shadow: 0 4px 6px -1px rgba(0,0,0,0.1);
		}

		.invoice-header {
			padding: 3rem;
			background-color: var(--background-gray);
		}

		.invoice-details {
			padding: 2rem;
		}

		.header-details {
			display: flex;
			flex-direction: column;
			gap: 3rem;
			justify-content: space-between;
			align-items: flex-start;
		}

		.company-name{
			color: var(--primary-color);
			letter-spacing: -0.025em;
			margin: 0;
			font-size: 1.6rem;
			font-weight: 800;
		}
		.company-details {
			width: 100%;
			display: flex;
			gap: 0.5rem;
			justify-content: space-between;
			align-items: flex-start;
		}

		.invoice-title {
			letter-spacing: -0.025em;
			line-height: 1;
			margin: 0;
			font-size: 2rem;
			font-weight: 600;
		}

		.client-logo {
			max-width: 100px;
			height: auto;
			border-radius: 1rem;
		}

		.meta-info {
			display: flex;
			flex-direction: column;
			gap: 0.25rem;
		}
		
		.table-container {
			margin: 0;
			border-radius: 0.5rem;
			overflow: hidden;
		}

		table {
			width: 100%;
			border-collapse: separate;
			border-spacing: 0;
		}

		th {
			padding: 0.75rem;
			text-align: left;
			border-bottom: 1px solid var(--border-color);
			color: var(--secondary-color);
			font-size: 1rem;
		}

		td {
			padding: 1rem;
			border-bottom: 1px solid var(--border-color);
			font-size: 0.9rem;
			font-weight: 500;
		}

		.amount-col-header{
			font-size: 0.9rem;
			font-weight: 500;
		}

		.secondary-item-title{
			color: var(--secondary-color);
		}

		.item-col-header{
			font-size: 0.9rem;
			font-weight: 500;
		}

		.amount-col {
			font-weight: 500;
		}

		.grand-total {
			font-size: 1.2rem;
			font-weight: 700;
			color: var(--primary-color);
			padding-top: 1.5rem;
			margin-top: 1.5rem;
		}

		.footer-notes {
			padding: 1rem;
			text-align: center;
			background-color: var(--primary-color);
			color: white;
			font-size: 1rem;
			margin: 0;
		}

		.meta-info-grid {
			display: flex;
			flex-direction: column;
			gap: 0.5rem;
			width: 100%;
		}

		.detail-row {
			display: flex;
			font-size: 0.8rem;
			flex-direction: column;
			gap: 0.25rem;
			margin-bottom: 0.5rem;
		}

	</style>
</head>
